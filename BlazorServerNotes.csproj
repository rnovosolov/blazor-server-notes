    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>b6c9ec00-69bc-44bd-b42a-0f52c8d7cc05</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="bootstrap" Version="5.3.2" />
